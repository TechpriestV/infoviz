  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "main": "./build/main.js",
  "scripts": {
    "start": "node ./build/main.js",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
